Checks: >
  -*
  readability-identifier-naming

CheckOptions:
  - { key: readability-identifier-naming.NamespaceCase, value: PascalCase }
  - { key: readability-identifier-naming.ClassCase, value: PascalCase }
  - { key: readability-identifier-naming.FunctionCase, value: PascalCase }
  - { key: readability-identifier-naming.MethodCase, value: PascalCase }
  - { key: readability-identifier-naming.VariableCase, value: camelBack }
  - { key: readability-identifier-naming.MemberPrefix, value: m_ }